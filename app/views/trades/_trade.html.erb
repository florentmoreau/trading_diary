<tr>
  <th><%= trade.trading_value %></th>
  <th><%= trade.category %></th>
  <th><%= trade.size %></th>
  <th><%= trade.open_price %></th>
  <th><%= trade.close_price %></th>
  <th><%= trade.open_date %></th>
  <th><%= trade.close_date %></th>
  <th><%= trade.profit %></th>
  <th><%= trade.comment %></th>
  <th><%= link_to "Edit", edit_journal_trade_path(trade), class: "btn btn-default" %></th>
  <th><%= link_to "Delete", journal_trade_path(trade), method: :delete, class: "btn btn-default" %></th>
</tr>



